<?xml version="1.0" encoding="UTF-8"?>
<faces-config
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
        http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd" version="2.0">

    <navigation-rule>
        <from-view-id>*</from-view-id><!--* означает для всех страничек-->

        <navigation-case>
            <from-outcome>goToAbout</from-outcome>
            <to-view-id>/About.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>goToMain</from-outcome>
            <to-view-id>/Main.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>goToHistory</from-outcome>
            <to-view-id>/History.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>goToPictures</from-outcome>
            <to-view-id>/Pictures.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>goToProfile</from-outcome>
            <to-view-id>/Profile.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>goToHomePage</from-outcome>
            <to-view-id>/HomePage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>goToInvalidPage</from-outcome>
            <to-view-id>/InvalidPage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>goQuestion1</from-outcome>
            <to-view-id>/question1.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>goQuestion2</from-outcome>
            <to-view-id>/question2.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>goQuestion3</from-outcome>
            <to-view-id>/question3.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>